<h1>My articles</h1>

<div class="p-3 m-4 border-bottom">
  <ul class="list-group list-group-flush">
    <% @articles.each do |article| %>
      <li class="list-group-item">
        <h3><%= link_to article.title, article_path(article) %></h3>
        <p><%= article.content %></p>
      </li>
    <% end %>
  </ul>
</div>

